  2: |
    apiVersion: gateway.networking.k8s.io/v1
    kind: HTTPRoute
    metadata:
      labels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: exposed-app
        app.kubernetes.io/part-of: homeserver
        app.kubernetes.io/version: 0.1.0
        helm.sh/chart: homeserver-app-0.1.0
        homeserver/vpn: "false"
      name: exposed-app
      namespace: NAMESPACE
    spec:
      hostnames:
        - app.example.com
        - application.example.com
      parentRefs:
        - name: gateway
          namespace: NAMESPACE
          sectionName: https
      rules:
        - backendRefs:
            - name: exposed-app
              port: 8080
          filters:
            - extensionRef:
                group: traefik.io
                kind: Middleware
                name: security-chain
              type: ExtensionRef
