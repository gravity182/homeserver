should handle all probes disabled:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      annotations: null
      labels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: no-probes-app
        app.kubernetes.io/part-of: homeserver
        app.kubernetes.io/version: 0.1.0
        helm.sh/chart: homeserver-app-0.1.0
        homeserver/vpn: "false"
      name: no-probes-app
      namespace: NAMESPACE
    spec:
      replicas: 1
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app.kubernetes.io/instance: RELEASE-NAME
          app.kubernetes.io/name: no-probes-app
      strategy:
        type: Recreate
      template:
        metadata:
          annotations: null
          labels:
            app.kubernetes.io/instance: RELEASE-NAME
            app.kubernetes.io/managed-by: Helm
            app.kubernetes.io/name: no-probes-app
            app.kubernetes.io/part-of: homeserver
            app.kubernetes.io/version: 0.1.0
            helm.sh/chart: homeserver-app-0.1.0
            homeserver/vpn: "false"
        spec:
          affinity: null
          automountServiceAccountToken: false
          containers:
            - env:
                - name: TZ
                  value: UTC
                - name: KUBERNETES_POD_IP
                  valueFrom:
                    fieldRef:
                      fieldPath: status.podIP
              image: app:1.0
              imagePullPolicy: IfNotPresent
              name: no-probes-app
              ports:
                - containerPort: 8080
                  name: http
                  protocol: TCP
              resources: null
              securityContext:
                readOnlyRootFilesystem: false
                runAsNonRoot: false
                seLinuxOptions: {}
                seccompProfile:
                  type: RuntimeDefault
              volumeMounts:
                - mountPath: /tmp
                  name: empty-dir
                  subPath: tmp-dir
                - mountPath: /log
                  name: empty-dir
                  subPath: log-dir
          enableServiceLinks: false
          priorityClassName: NAMESPACE-normal-priority
          securityContext:
            fsGroup: 1000
            fsGroupChangePolicy: Always
            supplementalGroups: []
            sysctls: []
          tolerations: null
          volumes:
            - emptyDir: {}
              name: empty-dir
