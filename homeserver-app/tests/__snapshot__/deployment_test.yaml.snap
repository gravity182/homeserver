should render complete deployment for standard HTTP app:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      annotations: null
      labels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: test-app
        app.kubernetes.io/part-of: homeserver
        app.kubernetes.io/version: 0.1.0
        helm.sh/chart: homeserver-app-0.1.0
        homeserver/vpn: "false"
      name: test-app
      namespace: NAMESPACE
    spec:
      replicas: 1
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app.kubernetes.io/instance: RELEASE-NAME
          app.kubernetes.io/name: test-app
      strategy:
        type: Recreate
      template:
        metadata:
          annotations: null
          labels:
            app.kubernetes.io/instance: RELEASE-NAME
            app.kubernetes.io/managed-by: Helm
            app.kubernetes.io/name: test-app
            app.kubernetes.io/part-of: homeserver
            app.kubernetes.io/version: 0.1.0
            helm.sh/chart: homeserver-app-0.1.0
            homeserver/vpn: "false"
        spec:
          affinity: null
          automountServiceAccountToken: false
          containers:
            - env:
                - name: TZ
                  value: UTC
                - name: KUBERNETES_POD_IP
                  valueFrom:
                    fieldRef:
                      fieldPath: status.podIP
              image: nginx:1.21
              imagePullPolicy: IfNotPresent
              livenessProbe:
                failureThreshold: 6
                httpGet:
                  path: /health
                  port: http
                initialDelaySeconds: 30
                periodSeconds: 10
                successThreshold: 1
                timeoutSeconds: 5
              name: test-app
              ports:
                - containerPort: 8080
                  name: http
                  protocol: TCP
              readinessProbe:
                failureThreshold: 6
                httpGet:
                  path: /ready
                  port: http
                initialDelaySeconds: 5
                periodSeconds: 10
                successThreshold: 1
                timeoutSeconds: 5
              resources: null
              securityContext:
                allowPrivilegeEscalation: false
                capabilities:
                  drop:
                    - ALL
                privileged: false
                readOnlyRootFilesystem: true
                runAsGroup: 1001
                runAsNonRoot: true
                runAsUser: 1001
                seLinuxOptions: {}
                seccompProfile:
                  type: RuntimeDefault
              volumeMounts:
                - mountPath: /tmp
                  name: empty-dir
                  subPath: tmp-dir
                - mountPath: /log
                  name: empty-dir
                  subPath: log-dir
          enableServiceLinks: false
          priorityClassName: NAMESPACE-normal-priority
          securityContext:
            fsGroup: 1000
            fsGroupChangePolicy: Always
            supplementalGroups: []
            sysctls: []
          tolerations: null
          volumes:
            - emptyDir: {}
              name: empty-dir
  2: |
    apiVersion: gateway.networking.k8s.io/v1
    kind: HTTPRoute
    metadata:
      labels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: test-app
        app.kubernetes.io/part-of: homeserver
        app.kubernetes.io/version: 0.1.0
        helm.sh/chart: homeserver-app-0.1.0
        homeserver/vpn: "false"
      name: test-app
      namespace: NAMESPACE
    spec:
      hostnames:
        - test-app.example.com
        - app.example.com
      parentRefs:
        - name: gateway
          namespace: NAMESPACE
          sectionName: https
      rules:
        - backendRefs:
            - name: test-app
              port: 8080
          filters:
            - extensionRef:
                group: traefik.io
                kind: Middleware
                name: security-chain
              type: ExtensionRef
  3: |
    apiVersion: v1
    kind: Service
    metadata:
      labels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: test-app
        app.kubernetes.io/part-of: homeserver
        app.kubernetes.io/version: 0.1.0
        helm.sh/chart: homeserver-app-0.1.0
        homeserver/vpn: "false"
      name: test-app
      namespace: NAMESPACE
    spec:
      ports:
        - name: http
          port: 8080
          protocol: TCP
          targetPort: http
      selector:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/name: test-app
      type: ClusterIP
